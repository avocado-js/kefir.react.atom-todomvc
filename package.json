{
  "name": "kral-todomvc",
  "private": true,
  "scripts": {
    "build": "export NODE_ENV=production && browserify src\/todomvc.js -t babelify -t envify -g uglifyify > public\/app.js",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run build",
    "watch": "watchify src\/todomvc.js -v -t [ babelify --sourceMaps=inline ] -o public\/app.js"
  },
  "license": "MIT",
  "dependencies": {
    "kefir": "^3.2.1",
    "kefir.atom": "^1.1.4",
    "kefir.react.html": "^1.1.2",
    "partial.lenses": "^1.1.1",
    "ramda": "^0.19.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "todomvc-app-css": "^2.0.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.6.5",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "envify": "^3.4.0",
    "eslint": "2.2.x",
    "eslint-plugin-react": "^4.2.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
