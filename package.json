{
  "name": "kral-todomvc",
  "private": true,
  "scripts": {
    "build": "export NODE_ENV=production && browserify src/todomvc.js -t babelify -t envify -g uglifyify > public/app.js",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run build",
    "watch": "watchify src/todomvc.js -v -t [ babelify --sourceMaps=inline ] -o public/app.js"
  },
  "license": "MIT",
  "dependencies": {
    "atom.storage": "^0.2.0",
    "kefir": "^3.2.1",
    "kefir.atom": "^2.2.1",
    "kefir.react.html": "^3.2.1",
    "partial.lenses": "^3.3.1",
    "ramda": "^0.21.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "todomvc-app-css": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "envify": "^3.4.0",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
