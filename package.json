{
  "name": "kral-todomvc",
  "private": true,
  "scripts": {
    "build": "export NODE_ENV=production && browserify src/todomvc.js -t babelify -t envify -g uglifyify > public/app.js",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run build",
    "watch": "watchify src/todomvc.js -v -t [ babelify --sourceMaps=inline ] -o public/app.js"
  },
  "license": "MIT",
  "dependencies": {
    "atom.storage": "^0.3.3",
    "kefir": "^3.3.0",
    "kefir.atom": "^2.2.2",
    "kefir.react.html": "^3.2.4",
    "partial.lenses": "^3.7.0",
    "ramda": "^0.21.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "todomvc-app-css": "^2.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "envify": "^3.4.1",
    "eslint": "^3.2.2",
    "eslint-plugin-react": "^6.0.0",
    "uglifyify": "^3.0.2",
    "watchify": "^3.7.0"
  }
}
